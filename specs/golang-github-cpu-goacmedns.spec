# Generated by go2rpm
%bcond_without check

# https://github.com/cpu/goacmedns
%global goipath         github.com/cpu/goacmedns
Version:                0.1.1

%gometa

%global common_description %{expand:
A Go library to handle acme-dns client communication and persistent account
storage.}

%global golicenses      LICENSE
%global godocs          README.md

Name:           %{goname}
Release:        14%{?dist}
Summary:        Go library to handle acme-dns client communication

License:        MIT
URL:            %{gourl}
Source0:        %{gosource}

%description
%{common_description}

%gopkg

%prep
%goprep

%build
for cmd in cmd/* ; do
  %gobuild -o %{gobuilddir}/bin/$(basename $cmd) %{goipath}/$cmd
done

%install
%gopkginstall
install -m 0755 -vd                     %{buildroot}%{_bindir}
install -m 0755 -vp %{gobuilddir}/bin/* %{buildroot}%{_bindir}/

%if %{with check}
%check
%gocheck
%endif

%files
%license LICENSE
%doc README.md
%{_bindir}/*

%gopkgfiles

