# Generated by go2rpm 1.3
%bcond_without check

# https://github.com/fvbommel/util
%global goipath         github.com/fvbommel/util
Version:                0.0.3

%gometa

# Remove in F36:
%global godevelheader %{expand:
Obsoletes:      golang-vbom-util-devel < 0-0.8
}

%global goaltipaths     vbom.ml/util

%global common_description %{expand:
Go utility packages.}

%global golicenses      LICENSE
%global godocs          README.md

Name:           %{goname}
Release:        13%{?dist}
Summary:        Go utility packages

License:        MIT
URL:            %{gourl}
Source0:        %{gosource}

%if %{with check}
# Tests
BuildRequires:  golang(github.com/bruth/assert)
%endif

%description
%{common_description}

%gopkg

%prep
%goprep
rm -rf sortorder

%build
for cmd in cmd/* ; do
  %gobuild -o %{gobuilddir}/bin/$(basename $cmd) %{goipath}/$cmd
done

%install
%gopkginstall
install -m 0755 -vd                     %{buildroot}%{_bindir}
install -m 0755 -vp %{gobuilddir}/bin/* %{buildroot}%{_bindir}/

%if %{with check}
%check
%gocheck
%endif

%files
%license LICENSE
%doc README.md
%{_bindir}/*

%gopkgfiles

