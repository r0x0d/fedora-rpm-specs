# Generated by go2rpm
# https://github.com/rcrowley/go-metrics/issues/249
%ifnarch s390x aarch64 ppc64le riscv64
%bcond_without check
%endif

# https://github.com/rcrowley/go-metrics
%global goipath         github.com/rcrowley/go-metrics
%global commit          cf1acfcdf4751e0554ffa765d03e479ec491cad6

%gometa

%global common_description %{expand:
Go port of coda hale's metrics library.}

%global golicenses      LICENSE
%global godocs          README.md memory.md

%global gosupfiles glide.lock glide.yaml

Name:           %{goname}
Version:        0
Release:        0.38%{?dist}
Summary:        Go port of coda hale's metrics library

# Automatically converted from old format: BSD - review is highly recommended.
License:        LicenseRef-Callaway-BSD
URL:            %{gourl}
Source0:        %{gosource}
Source1:        glide.yaml
Source2:        glide.lock

BuildRequires:  golang(github.com/stathat/go)

%description
%{common_description}

%gopkg

%prep
%goprep
cp %{S:1} %{S:2} .

%build
for cmd in cmd/* ; do
  %gobuild -o %{gobuilddir}/bin/$(basename $cmd) %{goipath}/$cmd
done

%install
%gopkginstall
install -m 0755 -vd                     %{buildroot}%{_bindir}
install -m 0755 -vp %{gobuilddir}/bin/* %{buildroot}%{_bindir}/

%if %{with check}
%check
%gocheck
%endif

%files
%license %{golicenses}
%doc %{godocs}
%{_bindir}/*

%gopkgfiles

