# Generated by go2rpm
%bcond_without check

# https://github.com/eknkc/amber
%global goipath         github.com/eknkc/amber
%global commit          cdade1c073850f4ffc70a829e31235ea6892853b

%gometa

%global common_description %{expand:
Amber is an elegant templating engine for Go Programming Language It is inspired
from HAML and Jade}

%global golicenses      LICENSE
%global godocs          README.md

Name:           %{goname}
Version:        0
Release:        0.25%{?dist}
Summary:        Elegant templating engine for Go Programming Language

License:        MIT
URL:            %{gourl}
Source0:        %{gosource}

%description
%{common_description}

%gopkg

%prep
%goprep

%build
for cmd in amberc; do
  %gobuild -o %{gobuilddir}/bin/$(basename $cmd) %{goipath}/$cmd
done

%install
%gopkginstall
install -m 0755 -vd                     %{buildroot}%{_bindir}
install -m 0755 -vp %{gobuilddir}/bin/* %{buildroot}%{_bindir}/

%if %{with check}
%check
%gocheck
%endif

%files
%license LICENSE
%doc README.md
%{_bindir}/*

%gopkgfiles

