# Generated by go2rpm 1
%bcond_without check

# https://github.com/tomnomnom/xtermcolor
%global goipath         github.com/tomnomnom/xtermcolor
Version:                0.1.2
%global tag             0.1.2

%gometa

%global common_description %{expand:
Find the closest xterm color to anything implementing color.Color.}

%global golicenses      LICENSE
%global godocs          examples README.mkd CHANGELOG.mkd

Name:           %{goname}
Release:        17%{?dist}
Summary:        Command to convert color.Colour to the nearest xterm/bash/shell color

License:        MIT
URL:            %{gourl}
Source0:        %{gosource}

%description
%{common_description}

%gopkg

%prep
%goprep

%build
for cmd in cmd/* ; do
  %gobuild -o %{gobuilddir}/bin/$(basename $cmd) %{goipath}/$cmd
done

%install
%gopkginstall
install -m 0755 -vd                     %{buildroot}%{_bindir}
install -m 0755 -vp %{gobuilddir}/bin/* %{buildroot}%{_bindir}/

%if %{with check}
%check
%gocheck
%endif

%files
%license %{golicenses}
%doc %{godocs}
%{_bindir}/*

%gopkgfiles

